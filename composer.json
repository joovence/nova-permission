{
    "name": "joovence/nova-permission",
    "description": "A Laravel Nova tool for Spatie's Permission library. Based on vyuldashev/nova-permission, adapted for use witn Nova 4",
    "keywords": [
        "laravel",
        "nova",
        "spatie-permission"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-permission": "^3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Joovence\\NovaPermission\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Joovence\\NovaPermission\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
